{
  "name": "hermes",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "^22.0.0",
    "@types/react": "^16.0.38",
    "@types/react-native": "^0.52.0",
    "@types/react-test-renderer": "^16.0.0",
    "babel-preset-react-native": "^4.0.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "jest-expo": "25.0.0",
    "react-devtools": "^3.2.1",
    "react-native-scripts-ts": "1.11.4",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "16.2.0",
    "ts-jest": "^22.0.4",
    "tslib": "^1.7.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.0"
  },
  "main": "./node_modules/react-native-scripts-ts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts-ts start",
    "eject": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "test": "node node_modules/jest/bin/jest.js",
    "lint": "eslint . --ext .ts,.tsx --ignore-pattern node_modules/",
    "devtools": "react-devtools"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-router-native)/"
    ],
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "@types/react-router-native": "^4.2.1",
    "expo": "^25.0.0",
    "react": "16.2.0",
    "react-native": "0.52.0",
    "react-native-elements": "^1.0.0-beta3",
    "react-router-native": "^4.2.0"
  }
}
